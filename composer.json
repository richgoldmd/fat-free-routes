{
  "name": "richardgoldstein/fat-free-routes",
  "description": "Generate FatFreeFramework Routes from DocBlocks",
  "license": "MIT",
  "keywords": [
    "fat free framework",
    "fatfree",
    "F3"
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ulrichsg/getopt-php": "^4",
    "phpdocumentor/reflection": "4.0.1",
    "phpdocumentor/reflection-docblock": "5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "RichardGoldstein\\FatFreeRoutes\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.x-dev",
    "roave/security-advisories": "dev-latest"
  },
  "bin": [
    "bin/f3routes"
  ]
}
